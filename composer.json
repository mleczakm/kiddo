        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/lock": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/scheduler": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.31.0",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/uid": "7.3.*",
        "symfony/ux-live-component": "^2.31.0",
        "symfony/ux-turbo": "^2.31.0",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/workflow": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfonycasts/tailwind-bundle": "^0.10.1",
        "tales-from-a-dev/flowbite-bundle": "^0.8.1",
        "twig/cssinliner-extra": "^3.23",
        "twig/extra-bundle": "^3.23.0",
        "twig/inky-extra": "^3.23",
        "twig/intl-extra": "^3.23.0",
        "twig/twig": "^3.23.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true,
        "platform": {
            "ext-xsl": "0.8.23"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "qa": {
            "Static analysis": "@sa",
            "Tests": "@tests"
        },
        "sa": {
            "Validate composer.json": "@composer validate",
            "Security audit": "@composer audit",
            "lint:twig": "bin/console lint:twig src templates",
            "Rector": "rector --dry-run",
            "Phpstan": "phpstan analyse --memory-limit=-1",
            "ECS": "ecs check"
        },
        "tests": {
            "Unit tests": "bin/phpunit --group unit",
            "Smoke": "@tests:smoke",
            "Functional": "@tests:functional"
        },
        "tests:smoke": {
            "Setup": "@tests:functional:setup",
            "Smoke tests": "bin/phpunit --group smoke"
        },
        "tests:functional": {
            "Setup": "@tests:functional:setup",
            "Functional tests": "bin/phpunit --group functional"
        },
        "tests:functional:setup": {
            "Functional tests setup": "bin/console doctrine:database:create --if-not-exists --env=test",
            "Functional tests schema": "bin/console doctrine:schema:update --force --env=test"
        },
        "qa:fix": [
            "@sa:fix"
        ],
        "sa:fix": {
            "Disable timeout": "Composer\\Config::disableProcessTimeout",
            "Rector": "rector",
            "ECS fix": "ecs check --fix",
            "Phpstan fix baseline step 1": "phpstan analyse --memory-limit=-1 --configuration=phpstan.regenerate-baseline.neon",
            "Phpstan fix baseline step 2": "phpstan analyse --memory-limit=-1 --generate-baseline"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.3.*"
        },
        "violinist": {
            "number_of_concurrent_updates": 2,
            "one_pull_request_per_package": 1,
            "bundled_packages": {
                "symfony/framework-bundle": [
                    "symfony/asset",
                    "symfony/asset-mapper",
                    "symfony/brevo-mailer",
                    "symfony/browser-kit",
                    "symfony/console",
                    "symfony/css-selector",
                    "symfony/debug-bundle",
                    "symfony/doctrine-messenger",
                    "symfony/dotenv",
                    "symfony/event-dispatcher",
                    "symfony/expression-language",
                    "symfony/form",
                    "symfony/framework-bundle",
                    "symfony/http-client",
                    "symfony/intl",
                    "symfony/lock",
                    "symfony/mailer",
                    "symfony/maker-bundle",
                    "symfony/mime",
                    "symfony/monolog-bundle",
                    "symfony/notifier",
                    "symfony/phpunit-bridge",
                    "symfony/process",
                    "symfony/property-access",
                    "symfony/property-info",
                    "symfony/runtime",
                    "symfony/scheduler",
                    "symfony/security-bundle",
                    "symfony/serializer",
                    "symfony/stimulus-bundle",
                    "symfony/stopwatch",
                    "symfony/string",
                    "symfony/translation",
                    "symfony/twig-bundle",
                    "symfony/uid",
                    "symfony/ux-live-component",
                    "symfony/ux-turbo",
                    "symfony/validator",
                    "symfony/web-link",
                    "symfony/web-profiler-bundle",
                    "symfony/workflow",
                    "symfony/yaml"
                ],
              "twig/twig": [
                "twig/intl-extra",
                "twig/inky-extra",
                "twig/extra-bundle",
                "twig/cssinliner-extra"
              ]
            },
            "blocklist": [
              "symfony/asset",
              "symfony/asset-mapper",
              "symfony/brevo-mailer",
              "symfony/browser-kit",
              "symfony/console",
              "symfony/css-selector",
              "symfony/debug-bundle",
              "symfony/doctrine-messenger",
              "symfony/dotenv",
              "symfony/event-dispatcher",
              "symfony/expression-language",
              "symfony/form",
