{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "name": "mleczakm/kiddo",
    "description": "App for bookings",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-ds": "*",
        "ext-iconv": "*",
        "ext-swoole": "*",
        "bgalati/monolog-sentry-handler": "^2.1",
        "brick/money": "^0.10.2",
        "directorytree/imapengine": "^1.15.5",
        "doctrine/dbal": "^3.10.1",
        "doctrine/doctrine-bundle": "^2.16.0",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.2",
        "dragonmantank/cron-expression": "^3.4",
        "dunglas/doctrine-json-odm": "^1.4.2",
        "logdash/php-sdk": "dev-main",
        "macpaw/symfony-health-check-bundle": "^1.7",
        "martin-georgiev/postgresql-for-doctrine": "^3.4.1",
        "novaway/feature-flag-bundle": "^3.0.0",
        "nyholm/psr7": "^1.8.2",
        "odolbeau/phone-number-bundle": "^4.2.1",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.3",
        "scienta/doctrine-json-functions": "^6.3",
        "sentry/sentry-symfony": "^5.3.1",
        "swoole-bundle/resetter-bundle": "^1.1",
        "swoole-bundle/swoole-bundle": "dev-master#211a324e8921dfaf9e46d0e0181a5d529d1f744c as 0.24.1",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/brevo-mailer": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/event-dispatcher": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2.8.2",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/lock": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/scheduler": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.30.0",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/uid": "7.3.*",
        "symfony/ux-live-component": "^2.30.0",
        "symfony/ux-turbo": "^2.30.0",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/workflow": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfonycasts/tailwind-bundle": "^0.10.1",
        "tales-from-a-dev/flowbite-bundle": "^0.8.1",
        "twig/cssinliner-extra": "^3.21",
        "twig/extra-bundle": "^3.21",
        "twig/inky-extra": "^3.21",
        "twig/intl-extra": "^3.21",
        "twig/twig": "^2.12|^3.21.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true,
        "platform": {
            "ext-xsl": "0.8.23"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "qa": {
            "Static analysis": "@sa",
            "Tests": "@tests"
        },
        "sa": {
            "Validate composer.json": "@composer validate",
            "Security audit": "@composer audit",
            "lint:twig": "bin/console lint:twig src templates",
            "Phpstan": "phpstan analyse --memory-limit=-1",
            "ECS": "ecs check"
        },
        "tests": {
            "Unit tests": "bin/phpunit --testsuite \"Unit\"",
            "Smoke": "@tests:smoke",
            "Functional": "@tests:functional"
        },
        "tests:smoke": {
            "Setup": "@tests:functional:setup",
            "Smoke tests": "bin/phpunit --testsuite \"Smoke\""
        },
        "tests:functional": {
            "Setup": "@tests:functional:setup",
            "Functional tests": "bin/phpunit --testsuite \"Functional\""
        },
        "tests:functional:setup": {
            "Functional tests setup": "bin/console doctrine:database:create --if-not-exists --env=test",
            "Functional tests schema": "bin/console doctrine:schema:update --force --env=test"
        },
        "qa:fix": [
            "@sa:fix"
        ],
        "sa:fix": {
            "Disable timeout": "Composer\\Config::disableProcessTimeout",
            "ECS fix": "ecs check --fix",
            "Phpstan fix baseline step 1": "phpstan analyse --memory-limit=-1 --configuration=phpstan.regenerate-baseline.neon",
            "Phpstan fix baseline step 2": "phpstan analyse --memory-limit=-1 --generate-baseline"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.3.*"
        },
        "violinist": {
            "automerge": 1,
            "automerge_method": "rebase",
            "one_pull_request_per_package": 1
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.3.1",
        "kodus/mock-cache": "^2.1",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.22",
        "phpstan/phpstan-doctrine": "^2.0.4",
        "phpstan/phpstan-symfony": "^2.0.7",
        "phpunit/phpunit": "^11.5.35",
        "rector/rector": "^2.1.4",
        "roave/security-advisories": "dev-latest",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.3.3",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",
        "symplify/easy-coding-standard": "^12.5.24",
        "zenstruck/foundry": "^2.6.3",
        "zenstruck/mailer-test": "^1.4.2",
        "zenstruck/messenger-test": "^1.11"
    }
}
