        "martin-georgiev/postgresql-for-doctrine": "^3.7.0",
        "novaway/feature-flag-bundle": "^3.0.1",
        "nyholm/psr7": "^1.8.2",
        "odolbeau/phone-number-bundle": "^4.2.5",
