{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "name": "mleczakm/kiddo",
    "description": "App for bookings",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-swoole": "*",
        "brick/money": "^0.10.1",
        "directorytree/imapengine": "^1.15.2",
        "doctrine/dbal": "^3.9.5",
        "doctrine/doctrine-bundle": "^2.15",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.0",
        "dragonmantank/cron-expression": "^3.4",
        "dunglas/doctrine-json-odm": "^1.4.2",
        "easycorp/easyadmin-bundle": "^4.24.9",
        "logdash/php-sdk": "dev-main#5aecf5d3cdb1f45e0a7e679c3e3d656f3e602134",
        "odolbeau/phone-number-bundle": "^4.1",
        "phpdocumentor/reflection-docblock": "^5.6.2",
        "phpstan/phpdoc-parser": "^2.1",
        "scienta/doctrine-json-functions": "^6.3",
        "swoole-bundle/swoole-bundle": "^0.24.1",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2.8.1",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/lock": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/scheduler": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.27.0",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/uid": "7.3.*",
        "symfony/ux-live-component": "^2.27.0",
        "symfony/ux-turbo": "^2.27.0",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/workflow": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfonycasts/tailwind-bundle": "^0.10.0",
        "tales-from-a-dev/flowbite-bundle": "^0.8.1",
        "twig/cssinliner-extra": "^3.21",
        "twig/extra-bundle": "^3.21",
        "twig/inky-extra": "^3.21",
        "twig/intl-extra": "^3.21",
        "twig/twig": "^2.12|^3.21.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true,
        "platform": {
            "ext-xsl": "0.8.23"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "qa": {
            "Static analysis": "@sa"
        },
        "sa": {
            "Validate composer.json": "@composer validate",
            "Security audit": "@composer audit",
            "lint:twig": "bin/console lint:twig src templates",
            "Phpstan": "phpstan analyse --memory-limit=-1",
            "ECS": "ecs check"
        },
        "tests": {
            "Unit tests": "bin/phpunit --testsuite \"Unit tests\"",
            "Smoke": "@tests:smoke",
            "All with coverage": "bin/phpunit --coverage-text --coverage-xml target/coverage"
        },
        "tests:smoke": {
            "Smoke tests": "bin/phpunit --testsuite \"Smoke tests\""
        },
        "qa:fix": {
            "Disable timeout": "Composer\\Config::disableProcessTimeout",
            "ECS fix": "ecs check --fix",
            "Phpstan fix baseline step 1": "phpstan analyse --memory-limit=-1 --configuration=phpstan.regenerate-baseline.neon",
            "Phpstan fix baseline step 2": "phpstan analyse --memory-limit=-1 --generate-baseline"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.3.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.3",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.17",
        "phpstan/phpstan-doctrine": "^2.0.3",
        "phpstan/phpstan-symfony": "^2.0.6",
        "phpunit/phpunit": "^11.5.26",
        "rector/rector": "^2.1.0",
        "roave/security-advisories": "dev-latest",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.3.1",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",
        "symplify/easy-coding-standard": "^12.5.20",
        "zenstruck/foundry": "^2.6.0"
    }
}
