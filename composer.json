        "dama/doctrine-test-bundle": "^8.6.0",
        "kodus/mock-cache": "^2.1",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "2.1.33",
        "phpstan/phpstan-doctrine": "^2.0.12",
        "phpstan/phpstan-symfony": "^2.0.12",
        "phpunit/phpunit": "^11.5.50",
        "rector/rector": "^2.3.1",
